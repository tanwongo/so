{{each result value i}}
<li class="clearfix">
<div class="ldv mt4">
<a href="#">
<span>{{value.name}}</span>
<span>{{value.code}}</span>
</a>
</div>
<div class="ldv">
<span class="dt fz12">{{value.price1}}</span>
<span class="bjdown"></span>
<span class="dt">{{value.price2}}</span>
</div>
<div class="ldv">
<span class="dt">-{{value.zd}}%</span>
<div class="bqdown">{{value.bq}}</div>
</div>
</li>
{{/each}}